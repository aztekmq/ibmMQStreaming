MQ_HOST=127.0.0.1
MQ_PORT=1414
MQ_QMGR=QM1
MQ_CHANNEL=DEV.APP.SVRCONN
MQ_USER=app
MQ_PASSWORD=passw0rd

# Optional CCDT path (leave empty to use HOST/PORT/CHANNEL).
# Example (Linux):
# MQ_CCDT=/home/user/ccdt/AMQCLCHL.TAB
MQ_CCDT=